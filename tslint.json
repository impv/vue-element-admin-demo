{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "semicolon": [true, "never"],
        "quotemark": [true, "single", "avoid-escape"],
        "arrow-parens": [true, "ban-single-arg-parens"],
        "interface-name": [true, "never-prefix"],
        "object-literal-sort-keys": false,
        "trailing-comma": [true, {
            "multiline": "never",
            "singleline": "never"
        }]
    },
    "rulesDirectory": []
}